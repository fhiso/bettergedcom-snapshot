  var editUrl = 'http://bettergedcom.wikispaces.com/page/edit/OrganizersMeetingNotes+2011-12-20+%28Dec+20%29?goto=http%3A%2F%2Fbettergedcom.wikispaces.com%2FOrganizersMeetingNotes%2B2011-12-20%2B%2528Dec%2B20%2529';

  jQuery('#js-edit-from-template').change(function() {
    window.location = ws.common.addUriParams(editUrl, [['template', jQuery(this).val()]]);
  });
});



--></script><!-- w4 83 tot:83:83(1) mem:4:19(19) wki:1:1(1) -->

</body>
</html>