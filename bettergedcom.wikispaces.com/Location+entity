    <h1 class="pageTitle"><a href="/Location+entity">Location entity</a></h1>
      <div class="btn-group pull-right">
          <span class="btn btn-default disabled tipme" title="you do not have permission to edit this page"><i class="fa fa-pencil fa-fw"></i> Edit</span>          <a class="btn btn-default tipme" title="view 71 discussion posts" href="/Location+entity#discussion"><i class="fa fa-comments fa-fw"></i> 71</a>          <a class="btn btn-default tipme" title="view 18 revisions" href="/page/history/Location+entity"><i class="fa fa-clock-o fa-fw"></i> 18</a>          <a class="btn btn-default ws-btn-more dropdown-toggle tipme" title="more options" href="/page/menu/Location+entity"><i class="icon-height-only fa-fw"></i>&hellip;</a>        <ul class="ws-more-menu dropdown-menu dropdown-menu-right">
